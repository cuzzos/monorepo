namespace shared {
    [Throws=CoreError]
    bytes process_event([ByRef] bytes msg);
    
    [Throws=CoreError]
    bytes view();
    
    [Throws=CoreError]
    bytes handle_response(u32 id, [ByRef] bytes res);
};

[Error]
enum CoreError {
    "SerializationError",
    "DeserializationError",
    "EventProcessingError",
};

